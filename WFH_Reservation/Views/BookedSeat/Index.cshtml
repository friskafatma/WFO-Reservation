<body>
    <input type="text" id="web_link" value="@Session["reserve_API"]" hidden />
    <input type="text" id="web_link2" value="@Session["emp_API"]" hidden />
    <input type="text" id="nrp_login" value="@Session["nrp"]" hidden />

    <div id="page-container" class="main-content-boxed">

        <!-- Main Container -->
        <main id="main-container">
            <!-- Page Content -->
            <div class="content content-boxed content-full overflow-hidden">
                <!-- Header -->
                <!-- Hero -->
                <div class="bg-primary">
                    <div class="bg-black-25">
                        <div class="content content-top text-center">
                            <div class="py-5">
                                <h1 class="fw-bold text-white mb-2">Booking Seat</h1>
                                <h2 class="h4 fw-normal text-white-75">Grab your seat now</h2>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- END Header -->
                <!-- Checkout -->
                <form action="op_checkout.html" method="POST" onsubmit="return false;">
                    <div class="row">
                        <!-- Order Info -->
                        <div class="col-xl-12">

                            <!-- Payment -->
                            <div class="block block-rounded">


                                <div class="block-content block-content-full">
                                    <p id="countdown"></p>
                                    <b>Section A</b>
                                    <div id="seatContainerA" class="block-content-full bg-body-light"></div>

                                    <br />
                                    <b>Section B</b>
                                    <div id="seatContainerB" class="block-content-full bg-body-light"></div>

                                </div>
                                <hr class="my-0">
                            </div>
                            <!-- Payment -->
                        </div>
                    </div>
                </form>
                <!-- END Checkout -->
            </div>
            <!-- END Page Content -->
        </main>
        <!-- END Main Container -->
    </div>
    <!-- Modal HTML -->
